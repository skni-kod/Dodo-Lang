type TestStruct {
    let mut i32 var1 = 0;
    let mut i32 var2 = 10;
    let mut i32 var3 = 10;

    TestStruct(let i32 var1, let i32 var2) {
        this.var1 = var1;
        this.var2 = var2;
        return;
    }

    ~TestStruct() {
        this.var1 = -1;
        return;
