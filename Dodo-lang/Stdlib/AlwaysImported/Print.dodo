// TODO: change these void once they actually work
void Print(let u64 length, let char* message) {
    syscall(1, 0, message, length);
    return;
}

void Println(let u64 length, let char* message) {
    syscall(1, 0, message, length);
    syscall(1, 0, "\n", 1);
    return;
}

